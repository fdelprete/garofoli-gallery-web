<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.delfilab.gargall">
	<namespace>Gargall</namespace>
	<entity name="Gargall" local-service="false" remote-service="true">
		<reference package-path="com.liferay.portal" entity="Group" />
		<reference package-path="com.liferay.portlet.dynamicdatalists" entity="DDLRecordSet" />
		<reference package-path="com.liferay.portlet.dynamicdatamapping" entity="DDMStructure" />
		<reference package-path="com.liferay.portlet.documentlibrary" entity="DLFileEntry" />
	</entity>
</service-builder>